{
  "name": "grip",
  "version": "0.4.74",
  "description": "Grip; meant to keep a grip on your stuff",
  "keywords": [
    "React",
    "TypeScript",
    "Grip",
    "Styled Components"
  ],
  "author": {
    "name": "Erik Versteeg",
    "email": "faralley.nl@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eversteeg/grip/issues"
  },
  "homepage": "https://github.com/eversteeg/grip#readme",
  "main": "src/index.tsx",
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "npm": ">=6.14.8",
    "node": ">=14.14.0"
  },
  "scripts": {
    "start": "npm run build:dev",
    "start-test": "npm run build:test",
    "build:dev": "cross-env NODE_ENV=development BABEL_ENV=dev webpack-dev-server --hot --config config/webpack.dev.js --mode development",
    "build:test": "cross-env NODE_ENV=development BABEL_ENV=dev webpack-dev-server --host 0.0.0.0 --disable-host-check --hot --config config/webpack.dev.js --mode development",
    "build:prod": "cross-env BABEL_ENV=prod webpack --config config/webpack.prod.js --mode production",
    "check-all": "npm run lint:css && npm run lint:js && npm run check-types",
    "check-types": "tsc",
    "dist": "npm run check-types && npm run build:prod",
    "dist:analyze": "npm run dist -- --env.analyze=true",
    "lint:css": "stylelint \"./src/**/*\"",
    "lint:js": "eslint \"./src/**/*\"",
    "prettier": "prettier --write **/*.{ts,tsx,js,css}"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@hot-loader/react-dom": "^17.0.1",
    "@types/node": "^16.11.7",
    "core-js": "^3.19.1",
    "currency.js": "^2.0.4",
    "faralley-ui-kit": "^1.0.0",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-loading-skeleton": "^2.2.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-svg": "^14.1.2",
    "react-table": "^7.7.0",
    "react-with-direction": "^1.3.1",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.0",
    "regenerator-runtime": "^0.13.9",
    "simple-crypto-js": "^3.0.0",
    "stream": "0.0.2",
    "styled-components": "^5.3.1",
    "styled-media-query": "^2.1.2",
    "styled-reset-advanced": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/react": "^17.0.35",
    "@types/react-dates": "^21.8.3",
    "@types/react-dom": "^17.0.11",
    "@types/react-google-recaptcha": "^2.1.2",
    "@types/react-helmet": "^6.1.3",
    "@types/react-redux": "^7.1.20",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-table": "^7.7.8",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.14",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^1.13.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.7",
    "dotenv-webpack": "7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-typescript-sort-keys": "^1.7.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.5.1",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-concentric-order": "^4.0.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-junit-formatter": "^0.2.2",
    "typescript": "^4.4.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.14.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
